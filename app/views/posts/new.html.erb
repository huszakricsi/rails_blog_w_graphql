<td><%= link_to 'Back to posts', posts_path %><td>
<br>
<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, placeholder: "title" %>
  <%= f.label :body %>
  <%= f.text_area :body, placeholder: "body" %>
  <%= f.label :tags %>
  <%= f.text_field :tags_attributes, placeholder: "tags" %>
  <%= f.submit "Create Post" %>
<% end %>
